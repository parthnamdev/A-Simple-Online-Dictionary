<%- include("partials/top"); -%>
  <body onload="hide_loading2()">
    <div class="load_div"><img class="load" src="Ball-1.3s-200px.svg" alt=""></div>
    <section class="mainBody">
    
    <section id="navigation">
      <nav class="navbar navbar-expand-lg">
        <a class="navbar-brand" href="/"
          ><span style="font-family: 'Ribeye Marrow', cursive;">D</span
          >ictionary.</a
        >
        <button
          class="navbar-toggler navbar-light"
          type="button"
          data-toggle="collapse"
          data-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav ml-auto">
            <li
              class="nav-item btn btn-dark col-lg-12 col-sm-4 mx-auto knowMoreButton"
            >
              <a class="nav-link" href="/know-more">Know more ></a>
            </li>
          </ul>
        </div>
      </nav>
    </section>
    <section id="searchBar">
      <div class="search-box mx-auto">
        <form method="post" action="/uploaded">
          <input
            method="post"
            name="input"
            action="/uploaded"
            class="input-box"
            type="text"
            id="word_input"
            autocomplete="off"
            placeholder="Search"
          />
          <a class="button" onclick="callPopUp()" href="#" id="searchbtn">
            <i class="fas fa-search"></i>
          </a>
        </form>
        <script>
          var input = document.getElementById("word_input");

          input.addEventListener("keyup", function (event) {
            // Number 13 is the "Enter" key on the keyboard
            if (event.keyCode === 13) {
              // Cancel the default action, if needed
              event.preventDefault();
              // Trigger the button element with a click
              document.getElementById("searchbtn").click();
            }
          });
        </script>
      </div>
    </section>
    <section id="tagLine">
      Wanna search a meaning ? ... Here you go !
    </section>
<%- include("partials/developers"); -%>
    <section id="popUp">
      <div class="card p-5 mx-auto">
        <span class="card-title pb-3"
          >Choose the type of algoritms :</span
        >
        <ul class="list-group list-group-flush m-auto">
          <li class="list-group-item">
            <div class="pretty p-icon p-round p-jelly edit">
              <input type="radio" name="algoradio" id="firstCheckBox" />
              <label for="firstCheckBox">Interpolation Search</label>
              <div class="state p-primary">
                <i class="icon mdi mdi-check" style="background-color: #2f3640;"></i>
              </div>
            </div>
          </li>
          <li class="list-group-item">
            <div class="pretty p-icon p-round p-jelly edit">
              <input type="radio" name="algoradio" id="secondCheckBox" />
              <label for="secondCheckBox">Jump Search</label>
              <div class="state p-primary">
                <i class="icon mdi mdi-check" style="background-color: #2f3640;"></i>
              </div>
            </div>
          </li>
          <li class="list-group-item">
            <div class="pretty p-icon p-round p-jelly edit">
              <input type="radio" name="algoradio" id="thirdCheckBox" />
              <label for="thirdCheckBox">Binary Search</label>
              <div class="state p-primary">
                <i class="icon mdi mdi-check" style="background-color: #2f3640;"></i>
              </div>
            </div>
          </li>
          <li class="list-group-item">
            <div class="pretty p-icon p-round p-jelly edit">
              <input type="radio" name="algoradio" id="fourthCheckBox" />
              <label for="fourthCheckBox">Linear Search</label>
              <div class="state p-primary">
                <i class="icon mdi mdi-check" style="background-color: #2f3640;"></i>
              </div>
            </div>
          </li>
        </ul>
        <button><a href="/output" class="btn btn-dark popUpButton mx-auto">Submit</a></button>
      </div>
    </section>
    <script src="file.js"></script>
<%- include("partials/bottom"); -%>